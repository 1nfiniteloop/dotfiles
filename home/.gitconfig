[user]
	email = gunnarsson.lars@gmail.com
	name = Lars Gunnarsson

[push]
	default = simple

[core]
	editor = vi
	#editor = gedit -s
	autocrlf = input

[alias]
	a = add
	b = branch
	c = commit
	ch = checkout
	d = diff
	l = log --oneline --graph --decorate
	la = log --oneline --graph --decorate --all
	ll = log
	m = merge
	s = status

[pull]
	rebase = true

[init]
	defaultBranch = main
